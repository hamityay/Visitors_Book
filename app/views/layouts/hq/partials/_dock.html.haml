%i#toggle.icon-align-justify.icon-large

%ul#dock

  %li.launcher
    %i.icon-dashboard
    =link_to  t('dock.dashboard'), hq_dashboard_index_path

  %li.launcher
    %i.fa.fa-road
    =link_to  t('dock.places'), hq_places_path

  %li.launcher
    %i.fa.fa-fire
    =link_to  t('dock.categories'), hq_categories_path

  %li.launcher
    %i.fa.fa-male
    = link_to t('activerecord.models.users'), hq_users_path

  %li.launcher
    %i.fa.fa-comments-o
    = link_to t('activerecord.models.comments'), hq_comments_path

  %li.launcher
    %i.fa.fa-bolt
    =link_to  t('dock.interests'), hq_interests_path

  %li.launcher
    %i.fa.fa-user-secret
    = link_to t('activerecord.models.admins'), hq_admins_path


  %br/
  %li.launcher.dropdown.hover
    %i.fa.fa-list
    %a{href: '#'}= t('dock.system_datas')
    %ul.dropdown-menu
      %li
        =link_to hq_countries_path do
          %i.fa.fa-database
          = t('activerecord.models.countries')
      %li
        =link_to hq_cities_path do
          %i.fa.fa-database
          = t('activerecord.models.cities')

